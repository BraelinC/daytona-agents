# Convex URL (get this from your Convex dashboard)
NEXT_PUBLIC_CONVEX_URL=https://your-project.convex.cloud

# Convex deployment name (for CLI)
CONVEX_DEPLOYMENT=prod:your-project-name

# Cloudflare Worker VNC Proxy URL
# Deploy the worker in cloudflare-worker/ first, then set this
NEXT_PUBLIC_VNC_PROXY_URL=https://vnc-proxy.your-account.workers.dev
